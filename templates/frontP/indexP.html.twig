    <body>
       
<section class="tables">
                    <div class="container-fluid">
                        <div class="row">


                            <div class="col-lg-12">
                                <div class="card">
                                    <div class="card-close">
                                        <div class="dropdown">
                                            <button type="button" id="closeCard3" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" class="dropdown-toggle"><i class="fa fa-ellipsis-v"></i></button>
                                            <div aria-labelledby="closeCard3" class="dropdown-menu dropdown-menu-right has-shadow"><a href="#" class="dropdown-item remove"> <i class="fa fa-times"></i>Close</a><a href="#" class="dropdown-item edit"> <i class="fa fa-gear"></i>Edit</a></div>
                                        </div>
                                    </div>
                                    <div class="card-header d-flex align-items-center">
                                        <h3 class="h4">Listes des patients</h3>
                                    </div>


                                 
                                    <div class="card-body">
                                        <div class="table-responsive">
                                            <table class="table table-striped table-hover">
                                                <thead>
                                                <div class="line"></div>
                                                <div class="form-group row">
                                                    <label class="col-sm-3 form-control-label">Trier par</label>
                                                    <div class="col-sm-9">
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <button data-toggle="dropdown" type="button" class="btn btn-outline-secondary dropdown-toggle">Action </button>
                                                                <div class="dropdown-menu"><a href="{{ path('listCINTrie') }}" class="dropdown-item">Trier par CIN</a>
                                                                    <div class="dropdown-divider"></div><a href="{{ path('listEmailTrie') }}" class="dropdown-item">Trier par Email</a>
                                                                </div>
                                                            </div>

                                                        </div>
                                                    </div>
                                                </div>

                                        <tr>
                                                    <th>Id</th>
                                                    <th>Cin</th>
                                                    <th>Nom</th>
                                                    <th>Prenom</th>
                                                    <th>Email</th>
                                                    <th>Password</th>
                                                    <th>Consultation</th>
                                            <th>Actions</th>


                                        </tr>
                                                </thead>

                                                <tbody>
                                                {% for patient in patients %}
                                                    <tr>
                                                        <td>{{ patient.id }}</td>
                                                        <td>{{ patient.cin }}</td>
                                                        <td>{{ patient.nom }}</td>
                                                        <td>{{ patient.prenom }}</td>
                                                        <td>{{ patient.email }}</td>
                                                        <td>{{ patient.password }}</td>
                                                        <td>{{ patient.consultation}}</td>


                                                        <td>
                                                            <button class="btn btn-lg btn-secondary"> <a href="{{ path('patient_show', {'id': patient.id}) }}">show</a></button>
                                                            <button class="btn btn-lg btn-secondary"><a href="{{ path('patient_edit', {'id': patient.id}) }}">edit</a></button>
                                                        </td>
                                                    </tr>
                                                {% else %}

                                                    <tr>
                                                        <td colspan="7">no records found</td>
                                                    </tr>
                                                {% endfor %}
                                                </tbody>
                                            </table>
                                            <div class="form-group">
                                            <button class="btn btn-lg btn-secondary"><a href="{{ path('pdf') }}">PDF</a></button>
                                                <button class="btn btn-lg btn-secondary"><a href="{{ path('patient_new') }}">Create new</a></button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>


                        </div>

